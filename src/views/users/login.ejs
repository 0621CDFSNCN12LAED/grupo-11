<!DOCTYPE html>
<html lang="en">
  <!-- Head -->
  <%- include("../partials/head") %>
  <body>
    <!-- Header -->
    <%- include("../partials/header") %>
    <!-- Main -->
    <main class="padding-16">
      <h1>Inicio</h1>
      <form method="POST">
        <div class="form-input">
          <label class="main-label" for="user-name">Nombre de usuario</label>
          <input
            id="user-name"
            type="text"
            name="user-name"
            placeholder="Nombre de usuario"
          />
        </div>

        <div class="form-input">
          <label class="main-label" for="password">Contraseña</label>
          <input
            id="password"
            type="text"
            name="password"
            placeholder="contraseña"
          />
        </div>

        <button type="submit">Enviar</button>
        <button type="reset">Resetear</button>
      </form>
    </main>
    <!-- footer -->
    <%- include("../partials/footer") %>
  </body>
</html>
